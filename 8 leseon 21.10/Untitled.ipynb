    "def rectangles_method(a, b, n):\n",
    "    result = 0\n",
    "    h = (b - a) / n\n",
    "    for j in range(n):\n",
    "        result += func(a + h * (j + 0.5))\n",
    "    result *= h\n",
    "    return result"
